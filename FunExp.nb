Cell[BoxData[
 FormBox[
  RowBox[{"\t", "\[IndentingNewLine]", "\t\t", 
   RowBox[{
    RowBox[{"action", "=", 
     RowBox[{"convertAction", "[", 
      RowBox[{
       RowBox[{"op", "[", 
        RowBox[{
         RowBox[{"b\[Psi]q", "[", 
          RowBox[{"q", ",", "w", ",", "a", ",", "\[Alpha]"}], "]"}], ",", 
         RowBox[{"\[Psi]q", "[", 
          RowBox[{
           RowBox[{"-", "q"}], ",", "x", ",", "a", ",", "\[Alpha]"}], "]"}]}],
         "]"}], " ", "+", " ", 
       RowBox[{"op", "[", 
        RowBox[{
         RowBox[{"b\[Psi]q", "[", 
          RowBox[{"q", ",", "w", ",", "a", ",", "\[Alpha]"}], "]"}], ",", 
         RowBox[{"b\[Psi]q", "[", 
          RowBox[{"s", ",", "y", ",", "c", ",", "\[Gamma]"}], "]"}], ",", 
         RowBox[{"\[Psi]q", "[", 
          RowBox[{"r", ",", "x", ",", "a", ",", "\[Beta]"}], "]"}], ",", 
         RowBox[{"\[Psi]q", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"-", "q"}], "-", "r", "-", "s"}], ",", "z", ",", "c", ",",
            "\[Delta]"}], "]"}]}], "]"}]}], "]"}]}], 
    "\[IndentingNewLine]"}]}], TraditionalForm]], "DisplayFormula",
