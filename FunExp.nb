Cell[BoxData[
 FormBox[
  RowBox[{"\t", "\[IndentingNewLine]", "\t\t", 
   RowBox[{
    RowBox[{"addIndices", "[", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"col", ",", " ", 
         RowBox[{"{", 
          RowBox[{
          "a", ",", " ", "b", ",", " ", "c", ",", " ", "d", ",", " ", "e", 
           ",", " ", "f"}], " ", "}"}]}], "}"}], ",", " ", 
