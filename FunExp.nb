Cell[CellGroupData[{

Cell["2.1.2. Action", "Subsubsection",
 CellChangeTimes->{{3.7626027903019447`*^9, 3.7626027967180643`*^9}, {
  3.762606784684456*^9, 
  3.762606788196444*^9}},ExpressionUUID->"9b720ed0-3dea-4b79-8aad-\
7eaad2052651"],

Cell["\<\
The action must be written in terms of the fields previously defined and all \
the indices must be contracted. The function convertAction[ ] must be used in \
order to let DoFun interpret this quantity as an action. In the QM model, we \
use:\
\>", "Text",
 CellChangeTimes->{{3.762606826204227*^9, 3.7626068573080587`*^9}, {
   3.762606904468553*^9, 3.762606977028707*^9}, 
   3.76260707503566*^9},ExpressionUUID->"43fafe95-10df-4da9-acc2-\
98cbd15bb3d9"],

Cell[BoxData[
 RowBox[{
  RowBox[{"action", "=", 
   RowBox[{"convertAction", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{"Z2", "*", "\[ImaginaryI]", " ", 
        RowBox[{"Module", "[", 
         RowBox[{
          RowBox[{"{", "\[Mu]", "}"}], ",", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"\[Mu]", "=", 
             RowBox[{"insDummy", "[", "]"}]}], "}"}], " ", ";", 
           RowBox[{
            RowBox[{"vec", "[", 
             RowBox[{"q", ",", "\[Mu]"}], "]"}], " ", 
            RowBox[{"\[Gamma]", "[", 
             RowBox[{"\[Mu]", ",", "w", ",", "x"}], "]"}]}]}]}], "]"}]}], 
       ")"}], 
      RowBox[{"op", "[", 
       RowBox[{
        RowBox[{"b\[Psi]q", "[", 
         RowBox[{"q", ",", "w", ",", "a", ",", "\[Alpha]"}], "]"}], ",", 
        RowBox[{"\[Psi]q", "[", 
         RowBox[{
          RowBox[{"-", "q"}], ",", "x", ",", "a", ",", "\[Alpha]"}], "]"}]}], 
       "]"}]}], "+", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{
      FractionBox["1", "2"], "*", 
      SuperscriptBox["m", "2"], "*", 
      RowBox[{"op", "[", 
       RowBox[{
        RowBox[{"\[Sigma]", "[", 
         RowBox[{"-", "q"}], "]"}], ",", 
        RowBox[{"\[Sigma]", "[", "q", "]"}]}], "]"}]}], "+", 
     "\[IndentingNewLine]", 
     RowBox[{
      FractionBox[
       RowBox[{"\[IndentingNewLine]", "1"}], "2"], "*", 
      SuperscriptBox["m", "2"], "*", 
      RowBox[{"op", "[", 
       RowBox[{
        RowBox[{"\[CapitalPi]", "[", 
         RowBox[{
          RowBox[{"-", "q"}], ",", "j"}], "]"}], ",", 
        RowBox[{"\[CapitalPi]", "[", 
         RowBox[{"q", ",", "j"}], "]"}]}], "]"}]}], "+", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{
      FractionBox["h", 
       SqrtBox[
        RowBox[{"2", "Nf"}]]], "*", 
      RowBox[{"op", "[", 
       RowBox[{
        RowBox[{"b\[Psi]q", "[", 
         RowBox[{"p", ",", "w", ",", "a", ",", "\[Alpha]"}], "]"}], ",", 
        RowBox[{"\[Psi]q", "[", 
         RowBox[{"q", ",", "w", ",", "a", ",", "\[Alpha]"}], "]"}], ",", 
        RowBox[{"\[Sigma]", "[", 
         RowBox[{
          RowBox[{"-", "p"}], "-", "q"}], "]"}]}], "]"}]}], "+", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"h", "*", "I", "*", 
      RowBox[{"\[Gamma]5", "[", 
       RowBox[{"w", ",", "x"}], "]"}], "*", 
      RowBox[{"Tf", "[", 
       RowBox[{"j", ",", "\[Alpha]", ",", "\[Beta]"}], "]"}], "*", 
      RowBox[{"op", "[", 
       RowBox[{
        RowBox[{"b\[Psi]q", "[", 
         RowBox[{"p", ",", "w", ",", "a", ",", "\[Alpha]"}], "]"}], ",", 
        RowBox[{"\[Psi]q", "[", 
         RowBox[{"q", ",", "x", ",", "a", ",", "\[Beta]"}], "]"}], ",", 
        RowBox[{"\[CapitalPi]", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"-", "p"}], "-", "q"}], ",", "j"}], "]"}]}], "]"}]}]}], 
    "\[IndentingNewLine]", "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.762606990438497*^9, 3.762607045221305*^9}, {
   3.7626071003182487`*^9, 3.762607120808075*^9}, {3.762607220607587*^9, 
   3.762607223327826*^9}, 3.762607278722994*^9, {3.762607356576268*^9, 
   3.7626073920369463`*^9}, {3.762607449866683*^9, 3.762607482321295*^9}, 
   3.7626084518446903`*^9, {3.762609756013544*^9, 3.762609768315589*^9}, {
   3.762610037285171*^9, 3.762610043276816*^9}, {3.7626101536066847`*^9, 
   3.7626101712451553`*^9}, {3.762610209344142*^9, 
   3.762610210532189*^9}},ExpressionUUID->"7738b382-814d-497a-9dd6-\
c9c333ba31d7"],

Cell["\<\
In our action, we include quark kinetic term, meson mass terms and Yukawa \
interaction. The antifermions are written always to the left of the operator. \
Notice that, in our convention, momentum dependence of all the fields adds to \
zero.\
\>", "Text",
 CellChangeTimes->{{3.762607080940641*^9, 3.762607088172679*^9}, {
  3.7626071289086113`*^9, 3.7626072157567873`*^9}, {3.76260728647304*^9, 
  3.7626073063735523`*^9}, {3.76261013992309*^9, 
  3.762610142507525*^9}},ExpressionUUID->"64e9542f-25ee-44bf-ac5f-\
